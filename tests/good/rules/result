### tuple ### reject any 22 192.168.0.1 any 0.0.0.0/0 in_eth0
-A ufw-user-input -i eth0 -p tcp -d 192.168.0.1 --dport 22 -j REJECT --reject-with tcp-reset
-A ufw-user-input -i eth0 -p udp -d 192.168.0.1 --dport 22 -j REJECT

### tuple ### reject any any 0.0.0.0/0 80 10.0.0.1 in_eth0
-A ufw-user-input -i eth0 -p tcp -s 10.0.0.1 --sport 80 -j REJECT --reject-with tcp-reset
-A ufw-user-input -i eth0 -p udp -s 10.0.0.1 --sport 80 -j REJECT

### tuple ### reject any any 192.168.0.1 any 10.0.0.1 in_eth0
-A ufw-user-input -i eth0 -d 192.168.0.1 -s 10.0.0.1 -j REJECT

### tuple ### reject any 22 192.168.0.1 any 10.0.0.1 in_eth0
-A ufw-user-input -i eth0 -p tcp -d 192.168.0.1 --dport 22 -s 10.0.0.1 -j REJECT --reject-with tcp-reset
-A ufw-user-input -i eth0 -p udp -d 192.168.0.1 --dport 22 -s 10.0.0.1 -j REJECT

