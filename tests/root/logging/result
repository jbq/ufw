-A ufw6-user-logging-input -p udp --dport 23 -j RETURN
-A ufw6-user-input -p udp --dport 23 -j ufw6-user-logging-input
-A ufw6-user-input -p udp --dport 23 -j ACCEPT

### tuple ### allow_log udp 137,138 ::/0 any ::/0 Samba - in
